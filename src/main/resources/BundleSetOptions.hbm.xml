<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.jabong.models">
	<class name="BundleSetOption" table="sku_bundle_sets_options">
		<id name="id" column="id_sku_bundle_sets_options">
			<generator class="native" />
		</id>
		<property name="fkSkuBundleSets" column="fk_sku_bundle_sets" />
		<property name="sku" column="sku" />
	</class>
</hibernate-mapping>
