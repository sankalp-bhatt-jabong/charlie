<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.jabong.models">
	<class name="SkuBundleMessage" table="sku_bundle_messages">
		<id name="id" column="id_sku_bundle_messages">
			<generator class="native" />
		</id>
		<property name="fkSkuBundle" column="fk_sku_bundle" />
		<property name="messages" column="messages" />
	</class>
</hibernate-mapping>
